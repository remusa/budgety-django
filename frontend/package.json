{
    "name": "frontend",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@emotion/core": "^10.0.14",
        "nprogress": "^0.2.0",
        "prop-types": "^15.7.2",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-router-dom": "^5.0.1",
        "react-scripts": "3.0.1",
        "react-spinners": "^0.5.5",
        "styled-components": "^4.3.2"
    },
    "devDependencies": {
        "@types/jest": "24.0.15",
        "@types/node": "12.6.3",
        "@types/query-string": "^6.3.0",
        "@types/react": "16.8.23",
        "@types/react-dom": "16.8.4",
        "@types/react-router-dom": "^4.3.4",
        "@types/styled-components": "^4.1.18",
        "cross-env": "^5.2.0",
        "node-sass": "^4.12.0",
        "typescript": "^3.5.3"
    },
    "scripts": {
        "start": "cross-env SKIP_PREFLIGHT_CHECK=true NODE_ENV=development react-scripts start",
        "build": "cross-env SKIP_PREFLIGHT_CHECK=true NODE_ENV=production react-scripts build",
        "eject": "react-scripts eject",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test": "npm run test:watch",
        "watch": "tsc --watch",
        "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
        "lint:quiet": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --quiet",
        "lint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix && echo 'Lint complete.'",
        "lint:fix2": "eslint . --fix && echo 'Lint complete.'",
        "format": "prettier \"src/**/*.{js,ts,html}\" --write && echo 'Format complete.'",
        "dev": "cross-env NODE_ENV=development npm start",
        "deploy": "npm run build && npm run firebase:deploy",
        "firebase:login": "firebase login",
        "firebase:init": "firebase init",
        "firebase:deploy": "firebase deploy"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "jestConfig": {
        "preset": "react-native-web"
    }
}
